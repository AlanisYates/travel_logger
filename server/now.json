{
  "version": 2,
  "name": "travel-log-ay",
  "builds": [
    {
      "src": "src/index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "src/index.js"
    }
  ],
  "alias": [
    "travel-log-ay"
   ],
   "env": {
    "NODE_ENV": "production",
    "PORT": "1337",
    "DATABASE_URL": "@travel-log-db",
    "CORS_ORIGIN": "https://travel-log-ay-cehdmzibi.now.sh/",
    "API_KEY": "@travel-log-api-key"
  }
}